    <project name="f2fs-tools" path="external/f2fs-tools" remote="fox" />
    <project path="external/busybox" name="android_external_busybox" remote="fox" revision="android-9.0" />
    <project path="vendor/qcom/opensource/commonsys" name="android_vendor_qcom_opensource_commonsys" remote="fox" revision="android-9.0" />
    

<!-- Include external Magisk repo for repack binaries -->
